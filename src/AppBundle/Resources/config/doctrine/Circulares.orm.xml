<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Circulares" table="circulares">
    <id name="ccircu" type="string" column="ccircu" length="10">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fcircu" type="date" column="fcircu" nullable="false"/>
    <field name="de" type="string" column="de" length="60" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="para" type="string" column="para" length="100" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="asunto" type="string" column="asunto" length="40" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="texto" type="text" column="texto" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
