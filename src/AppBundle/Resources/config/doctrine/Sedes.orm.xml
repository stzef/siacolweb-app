<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Sedes" table="sedes">
    <id name="csede" type="string" column="csede" length="2">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nsede" type="string" column="nsede" length="60" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="comment">Nombre Sede</option>
      </options>
    </field>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <field name="coddane" type="string" column="coddane" length="15" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="coddaneold" type="string" column="coddaneold" length="15" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="consesede" type="string" column="consesede" length="15" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
