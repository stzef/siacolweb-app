<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Modulos" table="modulos">
    <id name="cmodulo" type="integer" column="cmodulo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nmodulo" type="string" column="nmodulo" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="icono" type="string" column="icono" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <field name="cmodstring" type="string" column="cmodstring" length="6" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="backgroundimg" type="string" column="backgroundImg" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
