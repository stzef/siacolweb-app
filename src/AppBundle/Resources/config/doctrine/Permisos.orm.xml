<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Permisos" table="permisos">
    <indexes>
      <index name="codperfil" columns="codperfil"/>
      <index name="opcion" columns="opcion"/>
    </indexes>
    <id name="item" type="integer" column="item">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codperfil" type="integer" column="codperfil" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="opcion" type="integer" column="opcion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
