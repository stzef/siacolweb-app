<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Docreqalum" table="docreqalum">
    <indexes>
      <index name="calum" columns="calum"/>
      <index name="codcreq" columns="codcreq"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="calum" type="string" column="calum" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codcreq" type="string" column="codcreq" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="entregado" type="string" column="entregado" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
