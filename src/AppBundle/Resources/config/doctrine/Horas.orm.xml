<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Horas" table="horas">
    <id name="chora" type="string" column="chora" length="2">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nhora" type="string" column="nhora" length="40" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="hini" type="time" column="hini" nullable="false"/>
    <field name="hfin" type="time" column="hfin" nullable="false"/>
  </entity>
</doctrine-mapping>
