<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Bancos" table="bancos">
    <id name="cban" type="string" column="cban" length="2">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nban" type="string" column="nban" length="60" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="activo" type="string" column="activo" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
