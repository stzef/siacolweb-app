<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Asistencias" table="asistencias">
    <id name="casis" type="integer" column="casis">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ano" type="string" column="ano" length="4" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="peri" type="string" column="peri" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="mes" type="string" column="mes" length="2" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="cmate" type="string" column="cmate" length="6" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="calum" type="string" column="calum" length="8" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="fallas" type="integer" column="fallas" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
